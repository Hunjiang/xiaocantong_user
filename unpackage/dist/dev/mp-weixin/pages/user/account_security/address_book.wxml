<view><hx-navbar vue-id="f102f1c0-1" leftText="查看手机通讯录" defaultBackUrl="phone" fixed="{{true}}" bind:__l="__l"></hx-navbar><view class="content"><block wx:if="{{contacts.length>0||isSearch}}"><input class="search" type="text" placeholder="搜索联系人" data-event-opts="{{[['input',[['onSearchInput',['$event']]]]]}}" bindinput="__e"/></block><block wx:if="{{contacts.length>0}}"><view><scroll-view class="contact-scroll" scroll-y="{{true}}" scroll-into-view="{{scrollViewId}}"><block wx:for="{{contacts}}" wx:for-item="item" wx:for-index="key" wx:key="key"><view class="box"><view class="divider" id="{{item.letter}}"><text class="divider-text">{{item.letter}}</text></view><block wx:for="{{item.contacts}}" wx:for-item="contact" wx:for-index="index" wx:key="index"><view class="item" hover-class="hover" hover-start-time="20" data-event-opts="{{[['tap',[['onSelectClick',['$0'],[[['contacts','',key],['contacts','',index]]]]]]]}}" bindtap="__e"><image class="portrait" src="/static/img/missing-face.png" mode="aspectFill"></image><view class="item-content"><view class="text-black"><text>{{contact.name}}</text></view><view class="text-gray text-sm">{{''+appName+"："+contact.name+''}}</view></view></view></block></view></block></scroll-view><view class="indexBar-bg"><view class="indexBar"><view data-event-opts="{{[['touchstart',[['tStart',['$event']]]],['touchend',[['tEnd',['$event']]]],['touchmove',[['tMove',['$event']]]]]}}" class="indexBar-box" bindtouchstart="__e" bindtouchend="__e" bindtouchmove="__e"><block wx:for="{{contacts}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="indexBar-item" id="{{item.letter}}" data-event-opts="{{[['touchstart',[['getCur',['$event']]]],['touchend',[['setCur',['$event']]]]]}}" bindtouchstart="__e" bindtouchend="__e">{{''+item.letter+''}}</view></block></view></view></view><view hidden="{{!(!hidden)}}" class="indexToast">{{letter}}</view></view></block><block wx:else><text class="no-contact">通讯录暂无联系人</text></block></view></view>